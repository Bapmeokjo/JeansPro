<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.jeans.domain.calendar.dao">
<select id="selectCalendar" parameterType="String" resultType="com.project.jeans.domain.calendar.dto.CalendarDTO">
    SELECT * FROM events WHERE member_id = #{member_id}
</select>
<insert id="insertEvent" parameterType="com.project.jeans.domain.calendar.dto.CalendarDTO">
    <![CDATA[
    INSERT INTO events (title, start, end, member_id)
    VALUES (#{title}, #{start}, #{end}, #{member_id})"
    ]]>
</insert>
</mapper>