<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.jeans.domain.mypage.dao.EventDAO">
    <select id="selectMyPageAll" resultType="com.project.jeans.domain.mypage.dto.EventDTO">
        <![CDATA[
        select * from mypage;
        ]]>
    </select>

    <insert id="addEvent" parameterType="com.project.jeans.domain.mypage.dto.EventDTO">
        <![CDATA[
        INSERT INTO mypage (mypage_content, mypage_regdate, member_id)
        VALUES (#{mypage_content}, #{mypage_regdate}, #{member_id});
        ]]>
    </insert>
</mapper>